my:v_0_02

	a               :Version ;
	dc:issued       "2009-12-16"^^xsd:date ;
	:revision       "0.02"^^xsd:string ;
	:file-release   <http://backpan.perl.org/authors/id/T/TO/TOBYINK/HTML-HTML5-Parser-0.02.tar.gz> ;
